<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:stroom.widget.button.client">
    <g:FlowPanel styleName="max dock-container-vertical">
        <g:SimplePanel ui:field="content" styleName="dialog-content"/>
           <g:FlowPanel styleName="dialog-buttons-left dialog-buttons">
                <b:Button ui:field="create" addStyleNames="allow-focus Button--contained-green" text="New"/>
                <g:FlowPanel styleName="dialog-buttons-right">
                    <b:Button ui:field="ok" addStyleNames="allow-focus Button--contained-primary" text="OK"/>
                    <b:Button ui:field="cancel" addStyleNames="allow-focus Button--contained-secondary" text="Cancel"/>
                </g:FlowPanel>
            </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>
