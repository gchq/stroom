<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:p="urn:import:stroom.data.pager.client"
             xmlns:b="urn:import:stroom.widget.button.client"
             xmlns:form="urn:import:stroom.widget.form.client">
    <form:FormGroup ui:field="pagerFormGroup" addStyleNames="max">
        <g:FlowPanel addStyleNames="max dock-container-vertical">
            <g:FlowPanel ui:field="pagerContainer" styleName="pager dock-container-horizontal dock-min align-center">
                <b:ButtonPanel ui:field="buttonPanel" addStyleNames="dock-max"/>
                <p:Pager ui:field="pager" addStyleNames="dock-min"/>
            </g:FlowPanel>
            <g:SimplePanel ui:field="listContainer" addStyleNames="listContainer dock-max"/>
        </g:FlowPanel>
    </form:FormGroup>
</ui:UiBinder>
