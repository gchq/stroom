<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:i="urn:import:stroom.item.client"
             xmlns:v="urn:import:stroom.widget.tickbox.client.view"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form">
        <form:FormGroup identity="basicKeyValueInputSettingsId" label="Id">
            <g:Label ui:field="id" addStyleNames="w-100 stroom-control-label"/>
        </form:FormGroup>
        <form:FormGroup identity="basicKeyValueInputSettingsName" label="Name">
            <g:TextBox ui:field="name" addStyleNames="w-100"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder> 
