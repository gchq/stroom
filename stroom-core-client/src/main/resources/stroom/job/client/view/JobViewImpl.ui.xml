<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:SimplePanel styleName="max">
        <g:ThinSplitLayoutPanel styleName="max">
            <g:north size="400">
                <g:SimplePanel ui:field="jobList" styleName="max dashboard-panel"/>
            </g:north>
            <g:center>
                <g:FlowPanel styleName="max dashboard-panel form">
                    <form:FormGroup ui:field="jobNodeListFormGroup" addStyleNames="max">
                        <g:SimplePanel ui:field="jobNodeList" styleName="max dashboard-panel"/>
                    </form:FormGroup>
                </g:FlowPanel>
            </g:center>
        </g:ThinSplitLayoutPanel>
    </g:SimplePanel>
</ui:UiBinder>
