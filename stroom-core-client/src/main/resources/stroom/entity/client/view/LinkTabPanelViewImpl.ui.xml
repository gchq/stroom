<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:tab="urn:import:stroom.widget.tab.client.view">
    <g:FlowPanel styleName="max dock-container-vertical">
        <g:FlowPanel styleName="dock-min linkTabPanelViewImpl">
            <tab:LinkTabBar ui:field="tabBar" styleName="linkTabPanelViewImpl-middle linkTabBar"/>
        </g:FlowPanel>
        <g:FlowPanel ui:field="toolbarContainer" styleName="dock-min toolbarContainer"/>
        <tab:LayerContainerImpl ui:field="layerContainer" addStyleNames="dock-max"/>
    </g:FlowPanel>
</ui:UiBinder>
