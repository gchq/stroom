<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:SimplePanel styleName="max">
        <g:ThinSplitLayoutPanel styleName="max">
            <g:west size="400">
                <g:SimplePanel ui:field="credentialsList" styleName="max dashboard-panel"/>
            </g:west>
            <g:center>
                <g:SimplePanel ui:field="credentialsDetails" styleName="max dashboard-panel"/>
            </g:center>
        </g:ThinSplitLayoutPanel>
    </g:SimplePanel>
</ui:UiBinder>
