<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FlowPanel styleName="max form-padding form">
        <form:FormGroup identity="dictionarySettingsDescription" label="Description">
            <g:TextArea ui:field="description" width="400px"/>
        </form:FormGroup>
        <form:FormGroup identity="dictionarySettingsImports" label="Imports" addStyleNames="default-min-sizes flex-grow-1">
            <g:SimplePanel ui:field="imports" addStyleNames="dock-max stroom-control"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder>
