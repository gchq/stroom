<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:ThinSplitLayoutPanel unit="PX" vSplits="0.5" styleName="max">
        <g:north size="400">
            <g:SimplePanel ui:field="userList" styleName="max"/>
        </g:north>
        <g:center>
            <g:ThinSplitLayoutPanel unit="PX" hSplits="0.5" styleName="max">
                <g:west size="400">
                    <g:SimplePanel ui:field="parents" styleName="max"/>
                </g:west>
                <g:center>
                    <g:SimplePanel ui:field="children" styleName="max"/>
                </g:center>
            </g:ThinSplitLayoutPanel>
        </g:center>
    </g:ThinSplitLayoutPanel>
</ui:UiBinder>
