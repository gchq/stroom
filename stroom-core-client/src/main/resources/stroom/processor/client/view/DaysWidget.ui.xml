<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:tickbox="urn:import:stroom.widget.tickbox.client.view">
    <g:FlowPanel styleName="days">
        <tickbox:CustomCheckBox ui:field="mon" label="Monday"/>
        <tickbox:CustomCheckBox ui:field="tue" label="Tuesday"/>
        <tickbox:CustomCheckBox ui:field="wed" label="Wednesday"/>
        <tickbox:CustomCheckBox ui:field="thu" label="Thursday"/>
        <tickbox:CustomCheckBox ui:field="fri" label="Friday"/>
        <tickbox:CustomCheckBox ui:field="sat" label="Saturday"/>
        <tickbox:CustomCheckBox ui:field="sun" label="Sunday"/>
    </g:FlowPanel>
</ui:UiBinder>
