<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client"
             xmlns:tickbox="urn:import:stroom.widget.tickbox.client.view"
             xmlns:d="urn:import:stroom.widget.customdatebox.client">
    <g:FlowPanel styleName="max form-padding form">
        <form:FormGroup identity="pathwaysTraceStore" label="Trace Store">
            <g:SimplePanel ui:field="traceStore"/>
        </form:FormGroup>
        <form:FormGroup identity="pathwaysInfoFeed" label="Info Feed">
            <g:SimplePanel ui:field="infoFeed"/>
        </form:FormGroup>
        <form:FormGroup identity="temporalOrderingTolerance"
                        label="Temporal Ordering Tolerance">
            <d:DurationPicker ui:field="temporalOrderingTolerance"/>
        </form:FormGroup>
        <form:FormGroup identity="pathwaysInfoAllowPathwayCreation" label="Allow Pathway Creation">
            <tickbox:CustomCheckBox ui:field="allowPathwayCreation"/>
        </form:FormGroup>
        <form:FormGroup identity="pathwaysInfoAllowPathwayMutation" label="Allow Pathway Mutation">
            <tickbox:CustomCheckBox ui:field="allowPathwayMutation"/>
        </form:FormGroup>
        <form:FormGroup identity="pathwaysInfoAllowConstraintCreation" label="Allow Constraint Creation">
            <tickbox:CustomCheckBox ui:field="allowConstraintCreation"/>
        </form:FormGroup>
        <form:FormGroup identity="pathwaysInfoAllowConstraintMutation" label="Allow Constraint Mutation">
            <tickbox:CustomCheckBox ui:field="allowConstraintMutation"/>
        </form:FormGroup>
        <form:FormGroup identity="pathwaysProcessingNode" label="Processing Node">
            <g:TextBox ui:field="processingNode"/>
        </form:FormGroup>
    </g:FlowPanel>
</ui:UiBinder>