<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:q="urn:import:stroom.query.client.view"
             xmlns:b="urn:import:stroom.widget.button.client">
    <g:SimplePanel styleName="QueryToolbarViewImpl-outer">
        <g:FlowPanel styleName="QueryToolbarViewImpl-inner">
            <q:TimeRangeSelector ui:field="timeRangeSelector" addStyleNames="dock-min"/>
            <b:InlineSvgButton ui:field="errors" title="Show Errors"/>
            <q:QueryButtons ui:field="queryButtons" addStyleNames="dock-min"/>
        </g:FlowPanel>
    </g:SimplePanel>
</ui:UiBinder>
