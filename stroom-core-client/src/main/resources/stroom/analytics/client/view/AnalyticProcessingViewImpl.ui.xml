<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client"
             xmlns:l="urn:import:stroom.item.client">
    <g:FlowPanel styleName="max dock-container-vertical">
        <g:FlowPanel styleName="dock-min form-padding form">
            <form:FormGroup identity="analyticProcessingType" label="Processing Type">
                <l:SelectionBox ui:field="processingType"/>
            </form:FormGroup>
        </g:FlowPanel>
        <g:SimplePanel ui:field="processSettings" styleName="dock-max"/>
    </g:FlowPanel>
</ui:UiBinder> 
